USE Google_project;
SELECT
	DISTINCT customer_id
FROM
	google_project.customer_address;

SELECT
	 country
FROM 
	google_project.customer_address
WHERE
	LENGTH(country) > 2;

SELECT
	DISTINCT customer_id
FROM
	google_project.customer_address
WHERE
	SUBSTR(country,1,2) = 'US';

SELECT 
	state
FROM
	google_project.customer_address
WHERE
	LENGTH(state) > 2 ;

SELECT
	DISTINCT customer_id
FROM
	google_project.customer_address
WHERE
	TRIM(state) = 'OH';
    
